{% include 'incl/header.twig' %}

<body>
    <div class="grid wrapper">
        {% include 'incl/nav.twig' %}
        <div class="side-bar">
        </div>
        <div class="main landingpage">
            {{ content }}

            {% for page in pages("technology") %}
                <a href="{{ page.url }}">
                    <div class="technology box" + {{ page.class }}>
                        {{ page.title }}
                    </div>
                </a>
            {% endfor %}

        </div>
        {% include 'incl/footer.twig' %}
    </div>
</body>